<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jrsportsgame.jrbm.mapper.UserAuthMapper">

    <select id="getWXUserByOpenId" parameterType="java.lang.String" resultType="com.jrsportsgame.jrbm.entity.UserAuthEntity">
        select * from user_auth where auth_id=#{wxOpenId} and auth_type=1;
    </select>




</mapper>


